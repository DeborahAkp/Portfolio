<%- include('partials/head.ejs') %>
    <%- include('partials/navbar.ejs') %>

        <!-- Contact Me -->
        <form class="container-sm mt-5 mx-auto">
            <h1>Contact Me</h1>
            <div class="form-group">
                <label for="contactFirstName">First Name</label>
                <input type="text" class="form-control" id="contactFirstName" placeholder="First Name">
                <label for="contactLastName">Last Name</label>
                <input type="text" class="form-control" id="contactLastName" placeholder="Last Name">
            </div>
            <div class="form-group">
                <label for="contactMessage">Message</label>
                <textarea class="form-control" name="contactMessage" id="contactMessage" cols="20"
                    rows="10">Enter Message</textarea>
            </div>
            <button type="submit" class="btn btn-primary mt-3">Send Message</button>
        </form>
        </div>
        <%- include('partials/end.ejs') %>